{ 
  "routes": [ 
    {
      "route": "/api/*",
      "methods": ["POST", "DELETE"],
      "allowedRoles": [ "friends" ]
    }
  ],
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": [ "*.{jpg}" ]
  },
  "responseErrorOverrides": {
    "404": {
    }
  },
  "mimeTypes": { 
    ".jpg": "application/json"
  },
  "globalHeaders": {
    "a": "b"
  }
} 
