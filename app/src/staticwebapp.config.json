{ 
  "routes": [ 
    {
      "route": "/*.jpg",
      "redirect": "https://www.google.com"
    },
    {
      "route": "/logout",
      "rewrite": "/.auth/logout"
    },
    {
      "route": "/foo1",
      "redirect": "https://google.com/hi"
    },
    {
      "route": "/Foo1",
      "redirect": "/foo1"
    },
    {
      "route": "/bar",
      "statusCode": "301",
      "allowedRoles": ["admin"],
      "redirect": "/about"
    },
    {
      "route": "/baz",
      "redirect": "/about"
    },
    {
      "route": "/about/*",
      "allowedRoles": ["authenticated", "admin"],
      "rewrite": "/about"
    },
    {
      "route": "/admin",
      "rewrite": "/admin"
    },
    {
      "route": "/me",
      "redirect": "/.auth/me"
    },
    {
      "route": "/api/PostList",
      "methods": ["PUT", "POST"],
      "allowedRoles": ["authenticated"]
    }
  ],
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": [ "*.{jpg}" ]
  },
  "responseErrorOverrides": {
    "404": {
    }
  },
  "mimeTypes": { 
    ".jpg": "application/json"
  },
  "globalHeaders": {
    "a": "b"
  }
} 
